{
  "name": "Books",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "docker:up": "docker compose -f docker-compose.yml --env-file .env up -d",
    "prisma:setup": "npx prisma generate && npx prisma migrate dev --name init",
    "setup": "yarn docker:up && yarn prisma:setup ",
    "start:dev": "tsnd -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias",
    "start:prod": "yarn build && node ./dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "5.12.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-dist": "^3.30.4",
    "@types/swagger-ui-express": "^4.1.6",
    "prisma": "^5.12.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  }
}
